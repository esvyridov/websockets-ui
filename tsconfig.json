{
    "compilerOptions": {
      "lib": ["es2023"],
      "target": "es2022",
      "module": "node16",
      "moduleResolution": "node16",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"]
}